/**********************************************************************************
 ******************* 	guard of file will call on time in .c		***************
 **********************************************************************************/
#ifndef GPIO_CONFIG_H_
#define GPIO_CONFIG_H_

#define GPIO_ENABLE					1
#define GPIO_DISABLE				0

#define	GPIOA_CONFIG_EN				GPIO_DISABLE
#define	GPIOA_CONFIG_MODER			0x
#define GPIOA_CONFIG_OTYPER			0x
#define GPIOA_CONFIG_OSPEEDR		0x
#define GPIOA_CONFIG_PUPDR			0x
#define GPIOA_CONFIG_PUPDR			0x
#define GPIOA_CONFIG_IDR			0x
#define GPIOA_CONFIG_ODR			0x
#define GPIOA_CONFIG_BSRR			0x
#define GPIOA_CONFIG_LCKR			0x
#define GPIOA_CONFIG_AFR			0x






#endif /* GPIO_CONFIG_H_ */


