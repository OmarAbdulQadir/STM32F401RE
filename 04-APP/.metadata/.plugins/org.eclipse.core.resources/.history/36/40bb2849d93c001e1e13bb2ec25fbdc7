/***    			 Created By: Omar Abdul Qadir	 Version= 2.0	            ***/
/**********************************************************************************/
/***		         Note: All the data in the file is readable,				***/
/***			And any editing will affect the controller's behavior,			***/
/***				  So It's not recommended to edit any data,					***/
/***			Unless you are sure of what is the effect on behaviors.			***/
/**********************************************************************************/

/**********************************************************************************
 ******************* 	guard of file will call on time in .c		***************
 **********************************************************************************/
#ifndef GPIO_CONFIG_H_
#define GPIO_CONFIG_H_

/**********************************************************************************
 *******************		 		Configuration macros			***************
 **********************************************************************************/
#define GPIO_ENABLE					1
#define GPIO_DISABLE				0

/**********************************************************************************
 *******************		 		GPIOA configurations			***************
 **********************************************************************************/
#define	GPIOA_CONFIG_EN				GPIO_ENABLE
#define	GPIOA_CONFIG_MODER			0x00000000
#define GPIOA_CONFIG_OTYPER			0x00000000
#define GPIOA_CONFIG_OSPEEDR		0x00000000
#define GPIOA_CONFIG_PUPDR			0x00000000
#define GPIOA_CONFIG_ODR			0x00000000
#define GPIOA_CONFIG_BSRR			0x00000000
#define GPIOA_CONFIG_LCKR			0x00000000
#define GPIOA_CONFIG_AFR			0x0000000000000000

/**********************************************************************************
 *******************		 		GPIOB configurations			***************
 **********************************************************************************/
#define	GPIOB_CONFIG_EN				GPIO_ENABLE
#define	GPIOB_CONFIG_MODER			0x00000000
#define GPIOB_CONFIG_OTYPER			0x00000000
#define GPIOB_CONFIG_OSPEEDR		0x00000000
#define GPIOB_CONFIG_PUPDR			0x00000000
#define GPIOB_CONFIG_IDR			0x00000000
#define GPIOB_CONFIG_ODR			0x00000000
#define GPIOB_CONFIG_BSRR			0x00000000
#define GPIOB_CONFIG_LCKR			0x00000000
#define GPIOB_CONFIG_AFR			0x0000000000000000

/**********************************************************************************
 *******************		 		GPIOC configurations			***************
 **********************************************************************************/
#define	GPIOC_CONFIG_EN				GPIO_ENABLE
#define	GPIOC_CONFIG_MODER			0x00000000
#define GPIOC_CONFIG_OTYPER			0x00000000
#define GPIOC_CONFIG_OSPEEDR		0x00000000
#define GPIOC_CONFIG_PUPDR			0x00000000
#define GPIOC_CONFIG_IDR			0x00000000
#define GPIOC_CONFIG_ODR			0x00000000
#define GPIOC_CONFIG_BSRR			0x00000000
#define GPIOC_CONFIG_LCKR			0x00000000
#define GPIOC_CONFIG_AFR			0x0000000000000000

/**********************************************************************************
 *******************		 		GPIOD configurations			***************
 **********************************************************************************/
#define	GPIOD_CONFIG_EN				GPIO_ENABLE
#define	GPIOD_CONFIG_MODER			0x00000000
#define GPIOD_CONFIG_OTYPER			0x00000000
#define GPIOD_CONFIG_OSPEEDR		0x00000000
#define GPIOD_CONFIG_PUPDR			0x00000000
#define GPIOD_CONFIG_IDR			0x00000000
#define GPIOD_CONFIG_ODR			0x00000000
#define GPIOD_CONFIG_BSRR			0x00000000
#define GPIOD_CONFIG_LCKR			0x00000000
#define GPIOD_CONFIG_AFR			0x0000000000000000

/**********************************************************************************
 *******************		 		GPIOE configurations			***************
 **********************************************************************************/
#define	GPIOE_CONFIG_EN				GPIO_ENABLE
#define	GPIOE_CONFIG_MODER			0x00000000
#define GPIOE_CONFIG_OTYPER			0x00000000
#define GPIOE_CONFIG_OSPEEDR		0x00000000
#define GPIOE_CONFIG_PUPDR			0x00000000
#define GPIOE_CONFIG_IDR			0x00000000
#define GPIOE_CONFIG_ODR			0x00000000
#define GPIOE_CONFIG_BSRR			0x00000000
#define GPIOE_CONFIG_LCKR			0x00000000
#define GPIOE_CONFIG_AFR			0x0000000000000000

/**********************************************************************************
 *******************		 		GPIOH configurations			***************
 **********************************************************************************/
#define	GPIOH_CONFIG_EN				GPIO_ENABLE
#define	GPIOH_CONFIG_MODER			0x00000000
#define GPIOH_CONFIG_OTYPER			0x00000000
#define GPIOH_CONFIG_OSPEEDR		0x00000000
#define GPIOH_CONFIG_PUPDR			0x00000000
#define GPIOH_CONFIG_IDR			0x00000000
#define GPIOH_CONFIG_ODR			0x00000000
#define GPIOH_CONFIG_BSRR			0x00000000
#define GPIOH_CONFIG_LCKR			0x00000000
#define GPIOH_CONFIG_AFR			0x0000000000000000



#endif /* GPIO_CONFIG_H_ */


