#ifndef TIM_PRIVATE_H_
#define TIM_PRIVATE_H_

/**********************************************************************************
 ******************* 		Basic timers type struct				***************
 **********************************************************************************/
typedef struct{
	u32	CR1;
	u32	RESERVED_ADD1;
	u32	SMCR;
	u32	DIER;
	u32	SR;
	u32	EGR;
	u32	CCMR1;
	u32	RESERVED_ADD2;
	u32	CCER;
	u32	CNT;
	u32	PSC;
	u32	ARR;
	u32	RESERVED_ADD3;
	u32	CCR1;
	u32	CRR2;
}B_TIM_t;

typedef struct{

}G_TIMER_t;

typedef struct{

}A_TIMER_t;

#endif /* TIM_PRIVATE_H_ */
