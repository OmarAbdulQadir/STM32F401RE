#ifndef TIM_INTERFACE_H_
#define TIM_INTERFACE_H_


/**********************************************************************************
 ******************* 	TIM_vDelay_msc		***************
 **********************************************************************************/
void TIM_vDelay_ms(u16 );

/**********************************************************************************
 ******************* 	guard of file will call on time in .c		***************
 **********************************************************************************/
void TIM_vDelay_us(u16 );


#endif /* TIM_INTERFACE_H_ */
